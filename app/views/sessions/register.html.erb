<header>
  <h1>Library</h1>
</header>
<div class="row well">
  <%= form_for(@session, html:{class: 'form-horizontal'}) do |f| %>
    <%= render 'shared/errors', object: @session %>

    <%= bootstrap_form_field(f, :name) do %>
      <%= f.text_field :name, autofocus: true, class: "form-control" %>
    <% end %>

    <%= bootstrap_form_field(f, :user_id) do %>
      <%= f.text_field :user_id, class: "form-control" %>
    <% end %>

    <%= bootstrap_form_field(f, :password) do %>
      <%= f.password_field :password, class: "form-control" %>
    <% end %>

    <%= bootstrap_form_field(f, :password_confirmation) do %>
      <%= f.password_field :password_confirmation, class: "form-control" %>
    <% end %>

    <p>
      <%= f.submit class: 'btn btn-primary', value: 'Create an account' %>
    </p>
  <% end %>
</div>